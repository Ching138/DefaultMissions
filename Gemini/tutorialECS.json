            "Bool1": false,
            "ButtonId": 0,
            "Completed": false,
            "ComputerMode": 0,
            "Delivered": false,
            "DisplayTime": 3.0,
            "Fault": 0,
            "From": 2,
            "GaugeId": 0,
            "KnobID": 0,
            "Message": "The system is semi-automatic, but provides manual control when needed.",
            "PinID": 0,
            "PinPosition": 0,
            "String1": null,
            "String2": null,
            "String3": null,
            "ThresholdLimit": 0.0,
            "Timestamp": 0.0,
            "Value1": 0.0,
