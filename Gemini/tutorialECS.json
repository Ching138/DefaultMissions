            "From": 2,
            "GaugeId": 0,
            "KnobID": 0,
            "Message": "Welcome to the launch pad. Today we will stay grounded, but take a closer look at the Environmental Control System (ECS).",
            "PinID": 0,
            "PinPosition": 0,
            "String1": null,
            "String2": null,
            "String3": null,
            "ThresholdLimit": 0.0,
            "Timestamp": 5.0,
